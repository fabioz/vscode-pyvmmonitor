{
    "name": "vscode-pyvmmonitor",
    "displayName": "vscode-pyvmmonitor",
    "description": "Profile Python with PyVmMonitor",
    "version": "0.0.2",
    "publisher": "fabioz",
    "icon": "icon.png",
    "license": "MIT",
    "homepage": "https://github.com/fabioz/vscode-pyvmmonitor",
    "repository": {
        "type": "git",
        "url": "https://github.com/fabioz/vscode-pyvmmonitor"
    },
    "engines": {
        "vscode": "^1.18.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:vscode-pyvmmonitor.profile"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "vscode-pyvmmonitor.profile",
                "title": "Profile Python module with PyVmMonitor"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "ini": "^1.3.5",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}